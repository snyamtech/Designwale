<?xml version="1.0" encoding="utf-8"?>
<layout>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

<!--    <ImageView
        android:id="@+id/ivframebg"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:background="@drawable/frame_04"
        android:orientation="vertical" />-->

    <FrameLayout
        android:id="@+id/fl_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_3ssp">

            <ImageView
                android:id="@+id/iv_logo"
                android:layout_width="@dimen/_40ssp"
                android:layout_height="@dimen/_40ssp"
                android:layout_marginStart="@dimen/_3ssp"
                android:layout_marginTop="@dimen/_3ssp"
                android:scaleType="fitCenter"
                android:src="@mipmap/ic_launcher" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_logo_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center"
        android:layout_marginTop="@dimen/_15ssp">

        <LinearLayout
            android:id="@+id/linearName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:text="dasdads"
                android:textColor="@color/black_1000"
                android:textSize="@dimen/_9ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_name_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:padding="@dimen/_1ssp"
            android:gravity="center"
            android:orientation="horizontal">

            <FrameLayout
                android:id="@+id/fl_phone"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/linearPhone"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/iv_phone"
                        android:layout_width="@dimen/_10ssp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_2ssp"
                        android:padding="1dp"
                        android:src="@drawable/ic_call"
                        app:tint="@color/black_1000" />

                    <TextView
                        android:id="@+id/tv_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="9999999999"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_7ssp"/>

                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_phone_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />

            </FrameLayout>

            <FrameLayout
                android:id="@+id/fl_email"
                android:layout_weight="1"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/linearEmail"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2ssp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivEmail"
                        android:layout_width="@dimen/_10ssp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_2ssp"
                        android:padding="1dp"
                        android:src="@drawable/ic_email"
                        app:tint="@color/black_1000" />

                    <TextView
                        android:id="@+id/tv_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="24546@gmail.com "
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_7ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_email_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/fl_website"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3ssp">

                <LinearLayout
                    android:id="@+id/linearWebsite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_website"
                        android:layout_width="@dimen/_10ssp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_2ssp"
                        android:padding="1dp"
                        app:tint="@color/black_1000"
                        android:src="@drawable/ic_website" />

                    <TextView
                        android:id="@+id/tv_website"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="www.google.com"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_7ssp" />

                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_website_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>

        </LinearLayout>

        <FrameLayout
            android:id="@+id/fl_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#4c4c4c"
            android:paddingTop="@dimen/_2ssp"
            android:paddingBottom="@dimen/_2ssp"
            android:paddingStart="@dimen/_2ssp"
            android:paddingEnd="@dimen/_2ssp">

            <LinearLayout
                android:id="@+id/linearAddress"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:gravity="center">

                <ImageView
                    android:id="@+id/iv_address"
                    android:layout_width="@dimen/_10ssp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_2ssp"
                    android:padding="2dp"
                    android:src="@drawable/ic_address"
                    app:tint="@color/al_white" />

                <TextView
                    android:id="@+id/tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1ssp"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxLines="2"
                    android:text="9999999999"
                    android:textColor="@color/al_white"
                    android:textSize="@dimen/_7ssp" />

            </LinearLayout>

            <ImageView
                android:id="@+id/iv_address_close"
                android:layout_width="@dimen/_5ssp"
                android:layout_height="@dimen/_5ssp"
                android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                android:elevation="1dp"
                android:src="@drawable/ic_remove"
                android:visibility="gone" />

        </FrameLayout>

    </LinearLayout>

</FrameLayout>
</layout>