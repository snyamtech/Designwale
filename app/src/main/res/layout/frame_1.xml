<?xml version="1.0" encoding="utf-8"?>
<layout>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frameNew"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_frame_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/frame_1_img"
        android:scaleType="fitXY" />

    <FrameLayout
        android:id="@+id/fl_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginLeft="@dimen/_5ssp"
            android:layout_marginBottom="@dimen/_5ssp"
            android:visibility="visible">

            <ImageView
                android:id="@+id/iv_logo"
                android:layout_width="@dimen/_40ssp"
                android:layout_height="@dimen/_40ssp"
                android:layout_gravity="bottom"
                android:visibility="visible" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_logo_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_10ssp">

        <LinearLayout
            android:id="@+id/linearName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_2ssp"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:textColor="@color/black_1000"
                android:textSize="@dimen/_8ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_name_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />
    </FrameLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_8ssp"
        android:layout_gravity="bottom|center_vertical|center_horizontal|center"
        android:layout_marginLeft="@dimen/_1ssp"
        android:layout_marginBottom="@dimen/_25ssp"
        android:elevation="1dp"
        android:src="@drawable/ic_all_social"
        android:visibility="visible" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left|center_vertical|center_horizontal|center"
        android:layout_marginLeft="@dimen/_45ssp"
        android:layout_marginBottom="@dimen/_5ssp"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical|center_horizontal|center"
            android:orientation="horizontal">

            <FrameLayout
                android:id="@+id/fl_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <LinearLayout
                    android:id="@+id/linearPhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                   >

                    <ImageView
                        android:id="@+id/iv_phone"
                        android:layout_width="@dimen/_6ssp"
                        android:layout_height="@dimen/_6ssp"
                        android:layout_gravity="center"
                        android:layout_marginTop="1px"
                        android:background="@drawable/ic_call"
                        android:backgroundTint="@color/black_1000" />

                    <TextView
                        android:id="@+id/tv_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_6ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_phone_close"
                    android:layout_width="5dp"
                    android:layout_height="5dp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/fl_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <LinearLayout
                    android:id="@+id/linearEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <ImageView
                        android:id="@+id/ivEmail"
                        android:layout_width="@dimen/_6ssp"
                        android:layout_height="@dimen/_6ssp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/_2ssp"
                        android:layout_marginTop="1px"
                        backgroundTint="@color/black_1000"
                        android:background="@drawable/ic_email" />

                    <TextView
                        android:id="@+id/tv_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_6ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_email_close"
                    android:layout_width="5dp"
                    android:layout_height="5dp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/fl_website"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               >

                <LinearLayout
                    android:id="@+id/linearWebsite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <ImageView
                        android:id="@+id/iv_website"
                        android:layout_width="@dimen/_6ssp"
                        android:layout_height="@dimen/_6ssp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/_2ssp"
                        android:layout_marginTop="1px"
                        backgroundTint="@color/black_1000"
                        android:background="@drawable/ic_website" />

                    <TextView
                        android:id="@+id/tv_website"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_6ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_website_close"
                    android:layout_width="5dp"
                    android:layout_height="5dp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical|center_horizontal|center"
            android:orientation="horizontal">

            <FrameLayout
                android:id="@+id/fl_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <LinearLayout
                    android:id="@+id/linearLocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                   >

                    <ImageView
                        android:id="@+id/iv_address"
                        android:layout_width="@dimen/_6ssp"
                        android:layout_height="@dimen/_6ssp"
                        android:layout_gravity="center"
                        android:layout_marginTop="1px"
                        android:backgroundTint="@color/black_1000"
                        android:background="@drawable/ic_address" />

                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_1ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="2"
                        android:textColor="@color/black_1000"
                        android:textSize="@dimen/_6ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_address_close"
                    android:layout_width="5dp"
                    android:layout_height="5dp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>
</layout>